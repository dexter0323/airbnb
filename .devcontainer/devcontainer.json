{
  "name": "Airbnb Clone",
  "dockerComposeFile": "./docker-compose.yml",
  "service": "app",
  "postCreateCommand": "/workspace/.devcontainer/postCreate.sh",
  "workspaceFolder": "/workspace",
  "forwardPorts": [4321, 8080],
  "customizations": {
    "vscode": {
      "extensions": [
        "esbenp.prettier-vscode",
        "github.copilot",
        "github.copilot-chat",
        "github.vscode-github-actions"
      ]
    }
  }
}
